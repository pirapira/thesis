	author = {Berry, G\'{e}rard  and Boudol, G\'{e}rard},
	title = {The chemical abstract machine},
	journal = {Theo. Comp. Sci.},
	uuid = {6B371378-0A02-477D-8FDB-071A15347AA5},
	volume = {96},
	number = {1},
	pages = {217-248},
	year = {1992},
	ISSN = {0304-3975},
	sentelink = {file://localhost/Users/yh/sentelib.sente6lib/Contents/Attachments/Berry/1992/The%20chemical%20abstract%20machine.pdf,Sente,},
	Medium_consulted = {Web},
	Web_data_source = {ScienceDirect},
	publicationStatus = {Unknown},
	abstract = {We introduce a new kind of abstract machine based on the chemical metaphor used in the $\Gamma$ language of Ban\^{a}tre and Le M\'{e}tayer. States of a machine are chemical solutions where floating molecules can interact according to reaction rules. Solutions can be stratified by encapsulating subsolutions within membranes that force reactions to occur locally. We illustrate the use of this model by describing the operational semantics of the TCCS and CCS process calculi and of the fragment of Milner, Parrow and Walker's Calculus of Mobile Processes used by Milner to encode the lambda-calculus. We also give ideas on how to extract a higher-order concurrent $l$-calculus out of the basic concepts of the chemical abstract machine.},
	notes = {(0) <xocs:full-name>Selected Papers of the 2nd Workshop on Concurrency and Compositionality</xocs:full-name>}
}
